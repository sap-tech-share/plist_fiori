{
  "_version": "1.49.0",
  "sap.app": {
    "id": "project1",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.5",
      "toolsId": "fcb78a68-cc4f-444c-954d-83c8e3f954a8"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZPLIST_TP_V2_UI/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "attachService": {
        "uri": "/sap/opu/odata/sap/API_CV_ATTACHMENT_SRV",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localAttachService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "plist-manage": {
          "semanticObject": "plist",
          "action": "manage",
          "title": "{{flpTitle}}",
          "subTitle": "{{flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.117.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "project1.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "attachment": {
        "dataSource": "attachService",
        "preload": false,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "project1.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMainView",
          "pattern": ":?query:",
          "target": [
            "TargetMainView"
          ]
        },
        {
          "name": "RouteDetailView",
          "pattern": "Detail/{Plist}/{Action}",
          "target": [
            "TargetDetailView"
          ]
        },
        {
          "name": "RouteNewLicenseView",
          "pattern": "New",
          "target": [
            "TargetNewLicenseView"
          ]
        },
        {
          "name": "RouteNewCRView",
          "pattern": "NewCR",
          "target": [
            "TargetNewCRView"
          ]
        },
        {
          "name": "RouteUpdStatusView",
          "pattern": "StatusUpdate/{Plist}",
          "target": [
            "TargetUpdStatusView"
          ]
        }
      ],
      "targets": {
        "TargetMainView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetDetailView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Detail",
          "viewName": "Detail"
        },
        "TargetNewLicenseView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "NewLicense",
          "viewName": "NewLicense"
        },
        "TargetNewCRView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "NewCR",
          "viewName": "NewCR"
        },
        "TargetUpdStatusView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UpdStatus",
          "viewName": "UpdStatus"
        }
      }
    },
    "rootView": {
      "viewName": "project1.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "TargetDetailView": {
      "viewType": "XML",
      "transition": "slide",
      "clearControlAggregation": false,
      "viewId": "Detail",
      "viewName": "Detail"
    }
  }
}
